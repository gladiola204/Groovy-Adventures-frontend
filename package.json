{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@anatoliygatt/heart-switch": "^1.0.13",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fullpage/react-fullpage": "^0.1.40",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-transition-group": "^4.4.6",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "clsx": "^2.0.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^6.0.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "fullpage.js": "^4.0.20",
    "hamburger-react": "^2.5.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^10.1.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-day-picker": "^8.8.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-image-gallery": "^1.3.0",
    "react-modal": "^3.16.1",
    "react-quill": "^2.0.0",
    "react-rater": "^6.0.5",
    "react-redux": "^8.0.7",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.8",
    "react-stripe-checkout": "^2.6.3",
    "react-toastify": "^9.1.3",
    "react-transition-group-react-18": "^4.5.3",
    "redux-persist": "^6.0.0",
    "sass": "^1.63.2",
    "typescript": "^4.9.5",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:sass": "sass --no-source-map src/sass:public/css",
    "copy:assets": "copyfiles -u 1 ./src/assets/**/* public",
    "copy:html": "copyfiles -u 1 ./src/*.html public",
    "copy": "npm-run-all --parallel copy:*",
    "watch:assets": "onchange \"src/*.html\" -- npm run copy:assets",
    "watch:html": "onchange \"src/*.html\" -- npm run copy:html",
    "watch:sass": "sass --no-source-map --watch src/sass:public/css",
    "watch": "npm-run-all --parallel watch:*",
    "serve": "browser-sync start --server public --files public",
    "start": "npm-run-all build:sass copy:html && react-scripts start",
    "build": "npm-run-all build:sass copy:html && react-scripts build",
    "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r --no-map"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/draft-js": "^0.11.17",
    "@types/fullpage.js": "^2.9.3",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-image-gallery": "^1.2.0",
    "@types/react-modal": "^3.16.0",
    "@types/uuid": "^9.0.7"
  }
}
